{
  "name": "jurgenromeijn/wordpress-plugin-projects",
  "homepage": "https://github.com/jurgenromeijn/wordpress-plugin-projects",
  "description": "A Wordpress plugin that creates the portfolio/project post types and taxonomies and adds it to the menu.",
  "license": "GPL-3.0",
  "type": "wordpress-plugin",
  "keywords": [
    "WordPress", "plugins", "themes", "php", "projects"
  ],
  "authors": [
    {
      "name": "Jurgen Romeijn",
      "email": "jurgen.romeijn@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "require": {
    "php": ">=5.4",
    "johnpbloch/wordpress": "~4.5",
    "composer/installers": "~1.0",
    "symfony/dependency-injection": "~2.8",
    "symfony/config": "~2.8",
    "symfony/yaml": "~2.8"
  },
  "autoload": {
    "psr-4": {"JurgenRomeijn\\Projects\\": "src/"}
  },
  "extra": {
    "wordpress-install-dir": "wp"
  }
}
